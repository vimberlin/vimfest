<header id="home">
<%= partial "partials/header" %>
</header>

<% data.pages.each do |page| %>
<%
  title = page.keys[0]
  subtext = page[title]
%>
<% id = title.to_url %>
<div id="<%= id %>" class="block">
  <div class="container">
    <div class="row">
      <% if title.include?("Recap") %>
        <div class="hidden-xs">
      <% else %>
        <div class="col-xs-12">
      <% end %>
      <div class="col-xs-12">
        <div class="page-header">
          <h1>
            <%= title %>
            <% if subtext %>
            <small><%= subtext %></small>
            <% end %>
          </h1>
          <% if data.global.debug %>
          <a href="<%= api id %>" target="_self"><span class="label label-info">JSON API</span></a>
          <% end %>
        </div>
        <% if File.exists?("source/partials/_#{id}.erb") %>
          <%= partial "partials/#{id}" %>
        <% else %>
          <div class="alert alert-warning">
            <i class="fa fa-warning"></i>&nbsp;
            <strong>Warning:</strong> Partial <strong><%= id %></strong> does not exists.
            Create the file <code>/source/partials/_<%= id %>.erb</code>
            If you don't want this section, delete it from <code>/data/pages.yml</code> file.
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<% end %>

<footer id="footer">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
      </div>
    </div>
  </div>
</footer>
